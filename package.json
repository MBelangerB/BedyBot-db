{
  "name": "bedybot-db",
  "version": "2022.09.1",
  "description": "Database model and migration or BedyBot tools",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "build": "npx tsc",
    "eslint": "eslint .",
    "eslintFix": "eslint . --fix",
    "cleanState": "npm ci",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:seed": "npx sequelize-cli db:seed:all",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undoTo": "npx sequelize-cli db:migrate:undo:all --to XXXXXXXXXXXXXX.js",
    "migrate:create": "npx sequelize-cli migration:generate --name=",
    "package:pack": "npm pack --pack-destination ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MBelangerB/BedyBot-db.git"
  },
  "author": "Elmat90",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MBelangerB/BedyBot-db/issues"
  },
  "homepage": "https://github.com/MBelangerB/BedyBot-db#readme",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.18",
    "@types/validator": "^13.7.17",
    "dotenv": "^16.0.2",
    "eslint": "^8.13.0",
    "sequelize-cli": "^6.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "mysql2": "^3.4.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "uuid": "^9.0.0"
  }
}
