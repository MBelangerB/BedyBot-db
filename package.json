{
  "name": "bedybot-db",
  "version": "2023.06.1",
  "description": "Database model and migration for BedyBot tools",
  "main": "src/index.ts",
  "types": "./build/index.d.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "eslint": "eslint .",
    "eslintFix": "eslint . --fix",
    "cleanState": "npm ci",
    "typeorm:migrate": "typeorm migration:run -d src/bedydb-source.ts",
    "typeorm:newMigration": "typeorm migration:generate -n MyMigrationName",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.json",
    "package:pack": "npm pack --pack-destination ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MBelangerB/BedyBot-db.git"
  },
  "author": {
    "name": "Elmat90",
    "email": "mbelangerbeaulieu@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MBelangerB/BedyBot-db/issues"
  },
  "homepage": "https://github.com/MBelangerB/BedyBot-db#readme",
  "devDependencies": {
    "@types/node": "^20.3.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "uuid": "^9.0.0"
  },
  "dependencies": {
    "mysql2": "^3.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16"
  }
}
