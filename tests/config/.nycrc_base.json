{
    "check-coverage": true,
    "all": true,
    "include": [
        "src/controllers/**/*.js"
    ],
    "exclude": [
        "_tests_/**/*.*",
        "src/config/**/*.*",
        "src/migrations/**/*.*",
        "src/seeders/**/*.*",
        "src/oldMigration/**/*.*"
    ],
    "reporter": [
        "html",
        "lcov",
        "text",
        "text-summary"
    ],
    "report-dir": "coverage"
}