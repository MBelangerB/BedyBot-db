{
    "check-coverage": false,
    "all": true,
    "include": [
        "src/controllers/**/*.js"
    ],
    "exclude": [
        "_tests_/**/*.*",
        "src/config/**/*.*",
        "src/migrations/**/*.*",
        "src/seeders/**/*.*",
        "src/oldMigration/**/*.*"
    ],
    "reporter": [
        "json",
        "json-summary",
        "lcov",
        "text-summary"
    ],
    "report-dir": "coverageJson"
}